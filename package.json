    "@material-ui/lab": "4.0.0-alpha.49",
    "axios": "0.19.2",
    "bcryptjs": "2.4.3",
    "bootstrap": "4.4.1",
    "concurrently": "5.1.0",
    "connect-flash": "0.1.1",
    "contentful": "7.13.1",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "2.6.9",
    "ejs": "3.0.1",
    "express": "4.16.4",
    "express-ejs-layouts": "2.5.0",
    "express-session": "1.17.0",
    "google-maps-direction": "1.0.0",
    "google-maps-react": "2.0.2",
